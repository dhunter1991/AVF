var run=function(e){for(var o=require("ti.map"),a=Ti.UI.createWindow({backgroundColor:"#000"}),n=(o.createAnnotation({latitude:dataInfo.lat,longitude:dataInfo.lng,title:dataInfo.name,subtitle:dataInfo.location}),0),t=e.length;t>n;n++)var r=o.createView({top:0,height:350,mapType:o.STANDARD_TYPE,region:{latitude:e.lat,longitude:e.lng,latitudeDelta:.5,longitudeDelta:.5},animate:!0,regionFit:!0,userLocation:!1,width:Ti.UI.FILL});a.add(r),navWin.open()};exports.run=run;