var signUp=function(){var e=Ti.UI.createWindow({title:"sign up",backgroundColor:"#e8e5eb"}),o=Ti.UI.createImageView({image:"images/bg.jpg",width:"100%",height:"100%"}),t=Ti.UI.createTextField({hintText:" dhunter1@fullsail.edu",top:100,borderColor:"black",width:320,height:30,backgroundColor:"#fff"}),a=Ti.UI.createTextField({hintText:" password",top:t.top+75,borderColor:"black",width:320,height:30,passwordMask:!0,backgroundColor:"#fff"}),n=Ti.UI.createTextField({hintText:" confirm password",top:a.top+75,borderColor:"black",width:320,height:30,passwordMask:!0,backgroundColor:"#fff"}),i=Ti.UI.createButton({title:"cancel",right:20,width:150,height:30,bottom:50,borderRadius:5,backgroundColor:"red"}),r=Ti.UI.createButton({title:"Register",left:20,width:150,height:30,bottom:50,borderRadius:5,BackgroundColor:"green"});"android"===Ti.Platform.osname?(e.add(o),e.add(t),e.add(a),e.add(n),e.add(i),e.add(r),e.open()):(e.add(o,t,a,n,i,r),navWindow.openWindow(e)),exports.signUp=signUp;var d=function(){var o=(Ti.UI.createWindow({backgroundColor:"#fff",exitOnClose:!0,fullScreen:!1}),Ti.UI.createAlertDialog({message:"your account has been created!\n you can now sign in to view news updates.\n username: "+t.value,ok:"confirm",title:"account created"}));o.show(),e.close()};exports.confirmWin=d,r.addEventListener("click",d);var l=function(){e.close()};i.addEventListener("click",l),exports.closeWindow=l},logoutbutton=function(){var e=(Ti.UI.createWindow({backgroundColor:"#fff",exitOnClose:!0,fullScreen:!1}),Ti.UI.createAlertDialog({cancel:1,buttonNames:["continue","cancel"],message:"you have been logged out!",ok:"continue",title:"logout"}));e.show(),navWindow.close(),win.open(),navWindow.open()};exports.logoutbutton=logoutbutton,logout.addEventListener("click",logoutbutton),create.addEventListener("click",signUp);