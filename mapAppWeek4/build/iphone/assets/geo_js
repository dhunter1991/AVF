var geoFun=function(){if("android"===Ti.Platform.osname){var e=38.2454,o=-78.1108,a=require("modulemap");a.run(e,o)}else if(Ti.Geolocation.locationServicesEnabled)Ti.Geolocation.getCurrentPosition(function(e){if(e.error)console.log(e.error);else{console.log(e);var o=e.coords.latitude,a=e.coords.longitude,i=require("db");i.save(o,a)}});else{alert("No connection loading map from last location");var i=require("db");i.read(e,o)}};exports.geoFun=geoFun,signOn.addEventListener("click",geoFun);