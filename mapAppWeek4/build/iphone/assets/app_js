var win=Ti.UI.createWindow({title:"Location Map",backgroundColor:"#e8e5eb"}),image=Ti.UI.createImageView({image:"images/bg.jpg",width:"100%",height:"100%"}),logout=Ti.UI.createButton({title:"logout"});if("android"===Ti.Platform.osname);else var navWindow=Ti.UI.iOS.createNavigationWindow({window:win,rightNavButtons:logout});var user=Ti.UI.createTextField({hintText:" dhunter1@fullsail.edu",top:200,borderColor:"black",width:250,height:40,backgroundColor:"#fff"}),password=Ti.UI.createTextField({hintText:" password",top:250,borderColor:"black",width:250,height:40,passwordMask:!0,backgroundColor:"#fff"}),create=Ti.UI.createButton({title:"not a member? Register",width:200,height:50,color:"#fff",top:password.top+90}),signOn=Ti.UI.createButton({title:"log in",width:250,height:30,borderRadius:5,backgroundColor:"green",top:password.top+75});"android"===Ti.Platform.osname?(win.add(image),win.add(user),win.add(password),win.add(create),win.add(signOn),win.open()):(win.add(image),win.add(user),win.add(password),win.add(create),win.add(signOn),navWindow.open());var loadfile2=require("signup"),start=require("geo");start.geoFun();