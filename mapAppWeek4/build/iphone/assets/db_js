var dbArray=[],read=function(){console.log("read");var e=Ti.Database.open("locationInfo"),a=e.execute("SELECT lat, lng FROM asdTBL");if(a.rowCount>0){for(;a.isValidRow();){var o={lat:a.fieldByName("lat"),lng:a.fieldByName("lng")};dbArray.push(o),a.next()}var i=require("modulemap");i.run(dbArray)}else alert("no previous data")},save=function(e,a){console.log("save");var o=Ti.Database.open("locationInfo");o.execute("CREATE TABLE IF NOT EXISTS asdTBL (id INTEGER PRIMARY KEY, lat BLOB, lng BLOB)"),o.execute("INSERT INTO asdTBL (lat, lng) VALUES (?, ?)",e,a),o.close(),read()};exports.read=read,exports.save=save;