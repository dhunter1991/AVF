var run = function(dataArray){
	var MapModule = require('ti.map');
var annotations = MapModule.createAnnotation({
	latitude: dataArray.lat,
	longitude: dataArray.lng,
	title: dataArray.name,
	subtitle: dataArray.location,
	pincolor: MapModule.ANNOTATION_RED,
});
};
var pull = function (dbInfo){
	var MapModule = require('ti.map');
	var win2 = Ti.UI.createWindow({
	backgroundColor: "#000"
});
var mapview = MapModule.createView({
	top: 0,
	height: "100%",
    mapType: MapModule.STANDARD_TYPE,
    region: {latitude:dbInfo.lat, 
    		longitude:dbInfo.lng,
            latitudeDelta:0.5, 
            longitudeDelta:0.5},
    animate:true,
    regionFit:true,
    userLocation:true,
    annotations: run()
});
win2.add(mapview);
navWindow.openWindow(win2);
};
