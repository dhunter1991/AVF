var tbl=[],create=function(t,e){var i=Ti.Database.open("locationInfo");i.execute("CREATE TABLE IF NOT EXISTS asdTBL (id INTEGER PRIMARY KEY, city TEXT, state TEXT, temp TEXT, icon BLOB, feelslike TEXT, humidity TEXT, wind TEXT, forecast TEXT, weather TEXT, time TEXT, zip TEXT, gust TEXT, dew TEXT, heat TEXT, chill TEXT, visibility TEXT)");for(var o=i.execute("SELECT id, city, state, temp, icon, feelslike, humidity, wind, forecast, weather, time, zip, gust, dew, heat, chill, visibility");o.isValidRow();)tbl.push({id:o.fieldByName("id"),city:o.fieldByName("city"),state:o.fieldByName("state"),temp:o.fieldByName("temp"),icon:o.fieldByName("icon"),feelslike:o.fieldByName("feelslike"),humidity:o.fieldByName("humidity"),wind:o.fieldByName("wind"),forecast:o.fieldByName("forecast"),weather:o.fieldByName("weather"),time:o.fieldByName("time"),zip:o.fieldByName("zip"),gust:o.fieldByName("gust"),dew:o.fieldByName("dew"),heat:o.fieldByName("heat"),chill:o.fieldByName("chill"),visibility:o.fieldByName("visibility")}),o.next();console.log(tbl),o.close(),i.close();var a=require("ui");a.UiData(tbl)},update=function(t){var e=Ti.Database.open("locationInfo");e.execute("CREATE TABLE IF NOT EXISTS asdTBL (id INTEGER PRIMARY KEY, city TEXT, state TEXT, temp TEXT, icon TEXT, feelslike TEXT, humidity TEXT, wind TEXT, forecast TEXT, weather TEXT, time TEXT, zip TEXT, gust TEXT, dew TEXT, heat TEXT, chill TEXT, visibility TEXT)"),e.execute("delete FROM asdTBL"),e.execute("INSERT INTO asdTBL (city, state, temp, icon, feelslike, humidity, wind, forecast, weather, time, zip, gust, dew, heat, chill, visibility"),e.close(),read()};exports.create=create,exports.update=update;