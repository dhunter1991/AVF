var Ui=function(t){var o=Ti.UI.createWindow({backgroundColor:"#fff"}),e=Ti.UI.createImageView({image:"images/bg.jpg",width:"100%",height:"100%"}),i=Ti.UI.createImageView({image:t.icon,top:"30%",width:"10%",height:"10%"}),f=(Ti.UI.createLabel({color:"fff",text:t.city,width:"100%",top:"10%",font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"},textAlign:"center"}),Ti.UI.createLabel({color:"#fff",text:t.city+","+t.state,width:"100%",top:"10%",font:{fontFamily:"roboto",fontSize:40,fontWeight:"bold"},textAlign:"center"})),a=Ti.UI.createLabel({color:"#fff",text:t.temp+"° f",top:"30%",left:10,font:{fontFamily:"roboto",fontSize:50,fontWeight:"bold"},textAlign:"center"}),n=Ti.UI.createLabel({color:"#fff",text:"current Temp:",top:"28%",left:10,font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"},textAlign:"center"}),l=Ti.UI.createLabel({color:"#fff",text:"feels like: "+t.feelsLike+"° f",top:"36%",left:10,font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"},textAlign:"center"}),r=Ti.UI.createLabel({color:"#fff",text:"relative humidity: "+t.humidity,top:"57%",width:"100%",font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"},textAlign:"center"}),d=Ti.UI.createLabel({text:"wind speed: "+t.wind+" mph",color:"#fff",top:"28%",right:5,font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"}}),c=Ti.UI.createLabel({text:"Gust speeds: "+t.gust+" mph",color:"#fff",top:"31%",right:5,font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"}}),b=Ti.UI.createLabel({text:"heat index: "+t.heat,color:"#fff",top:"60%",font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"}}),h=Ti.UI.createLabel({text:"wind chill: "+t.chill,color:"#fff",top:"34%",right:5,font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"}}),g=Ti.UI.createLabel({text:"dew point: "+t.dew+"° f",color:"#fff",top:"63%",font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"}}),p=Ti.UI.createLabel({text:"visibility in miles: "+t.visibility,color:"#fff",top:"66%",font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold