var Api=function(e,t){console.log(Api);var o="http://api.wunderground.com/api/7e2a0de38337b6b0/conditions/hourly/forecast/q/"+e+","+t+".json";console.log(o);var i=Ti.Network.createHTTPClient({onload:function(e){var t=JSON.parse(this.responseText);console.log(t);var o={city:t.current_observation.display_location.city,state:t.current_observation.display_location.state,temp:t.current_observation.temp_f,icon:t.current_observation.icon_url,feelsLike:t.current_observation.feelslike_f,humidity:t.current_observation.relative_humidity,wind:t.current_observation.wind_mph,forecast:t.current_observation.forecast_url,weather:t.current_observation.weather,time:t.current_observation.observation_time,zip:t.current_observation.display_location.zip,gust:t.current_observation.wind_gust_mph,dew:t.current_observation.dewpoint_f,heat:t.current_observation.heat_index_string,chill:t.current_observation.windchill_f,visibility:t.current_observation.visibility_mi};console.log(o);var i=require("api");i.Ui(o)},onerror:function(e){alert("error has occured please check connection");var t=require("db");t.read()},timeout:5e3});i.open("GET",o),i.send()};exports.Api=Api;