var geoFun=function(){if("android"===Ti.Platform.osname){var e=38.2454,t=-78.1108,o=require("ui");o.Api(e,t)}else Ti.Geolocation.locationServicesEnabled?Ti.Geolocation.getCurrentPosition(function(e){if(e.error)console.log(e.error);else{console.log(e);var t=e.coords.latitude,o=e.coords.longitude,i=require("ui");i.Api(t,o)}}):alert("please enable location services to retreieve weather information")};exports.geoFun=geoFun;