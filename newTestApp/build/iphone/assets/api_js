var Ui=function(t){var e=Ti.UI.createWindow({backgroundColor:"#white"}),o=Ti.UI.createImageView({image:"images/back.png",width:"100%",height:"100%"}),i=Ti.UI.createLabel({color:"white",text:t.city,height:50,width:"100%",top:20,font:{fontFamily:"roboto",fontSize:30,fontWeight:"bold"},textAlign:"center"}),n=Ti.UI.createLabel({color:"white",text:t.state,height:50,width:"100%",top:70,font:{fontFamily:"roboto",fontSize:30,fontWeight:"bold"},textAlign:"center"}),r=Ti.UI.createLabel({color:"white",text:"tempreture: "+t.temp+" f",height:100,top:120,width:"50%",left:0,font:{fontFamily:"roboto",fontSize:25,fontWeight:"bold"},textAlign:"center"}),l=Ti.UI.createLabel({backgrounColor:"#9bd6ee",color:"white",text:"feels like: "+t.feelsLike+" f",height:100,top:120,width:"50%",right:0,font:{fontFamily:"roboto",fontSize:25,fontWeight:"bold"},textAlign:"center"}),a=Ti.UI.createLabel({backgroundcolor:"#9bd6ee",color:"white",text:"relative humidity "+t.humidity,height:100,top:270,width:"100%",font:{fontFamily:"roboto",fontSize:20,fontWeight:"bold"},textAlign:"center"}),h=Ti.UI.createLabel({text:"wind speed "+t.wind+" mph",color:"white",top:370}),c=Ti.UI.createImageView({image:t.icon,top:420});e.add(o,h,a,l,r,i,n,c),e.open()};exports.Ui=Ui;