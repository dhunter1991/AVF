var Api=function(e,t){console.log(Api);var o="http://api.wunderground.com/api/7e2a0de38337b6b0/conditions/q/"+e+","+t+".json";console.log(o);var i=Ti.Network.createHTTPClient({onload:function(e){var t=JSON.parse(this.responseText);console.log(t);var o={city:t.current_observation.display_location.city,state:t.current_observation.display_location.state,temp:t.current_observation.temp_f,icon:t.current_observation.icon_url,feelsLike:t.current_observation.feelslike_f,humidity:t.current_observation.relative_humidity,wind:t.current_observation.wind_mph};console.log(o);var i=require("api");i.Ui(o)},onerror:function(e){alert("error has occured please check connection")},timeout:5e3});i.open("GET",o),i.send()};exports.Api=Api;